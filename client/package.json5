{
  name: 'cubing-contests',
  type: 'module',
  private: true,
  scripts: {
    dev: "cp ../.env ./.env.local && next dev -p $(cat .env.local | grep -E '^NEXTJS_PORT=' | sed -r 's/^NEXTJS_PORT=([0-9]+).*/\\1/')",
    build: 'next build',
    start: 'next start',
    check: '@biomejs/biome lint && @biomejs/biome check',
    test: 'vitest run',
    'test:watch': 'vitest watch',
    'db:push': 'NODE_OPTIONS="--conditions=react-server" drizzle-kit push',
    'db:migrate': 'NODE_OPTIONS="--conditions=react-server" drizzle-kit migrate',
    'db:generate': 'NODE_OPTIONS="--conditions=react-server" drizzle-kit generate',
    'auth:migrate': '@better-auth/cli migrate',
    // BEFORE RUNNING THIS COMMENT OUT THE "relations" PROPERTY IN THE DRIZZLE CONFIG OBJECT IN provider.ts
    'auth:generate': '@better-auth/cli generate --output ./server/db/schema/auth-schema.ts',
  },
  dependencies: {
    '@cubing/icons': '3.0.5',
    '@fortawesome/fontawesome-svg-core': '7.1.0',
    '@fortawesome/free-regular-svg-icons': '7.1.0',
    '@fortawesome/free-solid-svg-icons': '7.1.0',
    '@fortawesome/react-fontawesome': '3.1.1',
    '@mdx-js/loader': '3.1.1',
    '@mdx-js/react': '3.1.1',
    '@next/mdx': '16.1.1',
    '@tanstack/react-virtual': '3.13.14',
    '@wca/helpers': '1.1.6',
    'better-auth': '1.4.10',
    bootstrap: '5.3.8',
    'country-flag-icons': '1.6.4',
    cubing: '0.61.2',
    'date-fns': '4.1.0',
    'date-fns-tz': '3.2.0',
    'drizzle-orm': '1.0.0-beta.8-734e789',
    'geo-tz': '8.1.4',
    handlebars: '4.7.8',
    lodash: '4.17.21',
    mailtrap: '4.4.0',
    next: '16.1.1',
    'next-safe-action': '8.0.11',
    pino: '10.1.0',
    'pino-logflare': '0.5.2',
    postgres: '3.4.7',
    react: '19.2.3',
    'react-datepicker': '9.1.0',
    'react-dom': '19.2.3',
    'react-markdown': '10.1.0',
    'remove-accents': '0.5.0',
    'server-only': '0.0.1',
    typescript: '5.9.3',
    zod: '4.3.4',
  },
  devDependencies: {
    '@biomejs/biome': '2.3.10',
    '@electric-sql/pglite': '0.3.14',
    '@next/env': '16.1.1',
    '@types/lodash': '4.17.21',
    '@types/mdx': '2.0.13',
    '@types/node': '25.0.3',
    '@types/pg': '8.16.0',
    '@types/react': '19.2.7',
    '@types/react-dom': '19.2.3',
    'drizzle-kit': '1.0.0-beta.8-734e789',
    vitest: '4.0.16',
  },
}
